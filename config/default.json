{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "application": {
    "arraySeparator": ","
  },
  "db": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "postgres",
    "schema": "polygon_parts",
    "synchronize": false,
    "logging": false,
    "entities": ["**/DAL/*.js"],
    "migrations": ["db/migrations/*.js", "db/migrations/*.ts"]
  }
}
